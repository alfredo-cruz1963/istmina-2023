<div class="row">
  <div class="col-md-12">
    <div class="card-header d-flex align-items-center" style="background-color: rgba(232, 240, 254, 0.90)">
      <i class="text-success mr-1"></i><strong>{{municipio}} - {{nombrePuesto}} - Mesas: {{totmesas}}</strong>
    </div>
  </div>
</div>

<div class="row mt-1">
  <div class="col-md-12 mx-auto">
    <div class="card ">
      <div class="card-body">
        <div class="table-responsive">
          <table id="tab-consulta" class="table table-bordered" style="width:100%" cellspacing="0">
            <thead class="text-white" style="background-color: #165a96">
              <tr>
                <td>Cod Mesa</strong></td>
                <td>Mesa</td>
                <td>Can1</td>
                <td>Can2</td>
                <td>Can3</td>
                <td>Can4</td>
                <td>Can5</td>
                <td>Can6</td>
                <td>Can7</td>
                <td>Blan</td>
                <td>Nulos</td>
                <td>Nom</td>
                <td>E-11</td>
                <td>Rect</td>
                <td>Recl</td>
                <td style=" background-color: #28a745;">Fot1</td>
                <td style=" background-color: #28a745;">Fot2</td>
                <td style=" background-color: #28a745;">Fot3</td>
                <td style=" background-color: #ffc107;">FotR</td>
              </tr>
            </thead>
            <tbody>
              {{#if mesaspuesto }}
              {{#each mesaspuesto }}
              <tr>
                <td>{{codigo}}</td>
                <td>{{mesa}}</td>
                <td>{{can1}}</td>
                <td>{{can2}}</td>
                <td>{{can3}}</td>
                <td>{{can4}}</td>
                <td>{{can5}}</td>
                <td>{{can6}}</td>
                <td>{{can7}}</td>
                <td>{{blancos}}</td>
                <td>{{nulos}}</td>
                <td>{{nomarcado}}</td>
                <td>{{sufragos}}</td>
                <td>{{recuento}}</td>
                <td>{{reclama}}</td>
                <td>{{foto}}</td>
                <td>{{foto2}}</td>
                <td>{{foto3}}</td>
                <td>{{fotrec}}</td>
              </tr>
              {{/each }}
              {{/if }}
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>