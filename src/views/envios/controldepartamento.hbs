<div class="container mt-1">
  <div class="row">
    <div class="col-md-12">
      <div class="card-header d-flex align-items-center" style="background-color: rgba(232, 240, 254, 0.90)">
        <i class="text-success mr-1"></i><strong class="municipio">Departamento del Meta</strong>
      </div>
    </div>
  </div>

  <div class="row mt-1">
    <div class="col-md-12 mx-auto">
      <div class="card ">
        <div class="card-body">
          <div class="table-responsive">
            <table id="tab-departamental" class="table table-bordered" style="width:100%" cellspacing="0">
              <thead class="text-white" style="background-color: #165a96">
                <td><strong>Codigo</strong></td>
                <td><strong>Municipio</strong></td>
                <td><strong>Tot Puestos</strong></td>
                <td><strong>Tot Mesas</strong></td>
                <td><strong>Mesas Report</strong></td>
                 <td><strong>% Report</strong></td>
                <td><strong>Total Votos</strong></td>
                <td><strong>Acci√≥n</strong></td>
                </tr>
              </thead>
              <tbody>
                {{#if mesasDpto }}
                {{#each mesasDpto }}
                <tr>
                  <td>{{codmpio}}</td>
                  <td>{{mpio}}</td>
                  <td>{{puestos}}</td>
                  <td>{{mesas}}</td>
                  <td>{{mesasreport}}</td>
                  <td>{{porc_report}}</td>
                  <td>{{totvot}}</td>
                  <td>
                    <a href="/envios/consulta/{{codmpio}}" class="btnEditar text-primary" title="Mostrar"><i class="fas fa-search"></i></a>
                  </td>
                </tr>
                {{/each }}
                {{/if }}
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>